{
  "name": "game-api-client",
  "version": "1.0.0",
  "description": "Cliente TypeScript para API do jogo usando TCP Socket com Little Endian",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "game",
    "api",
    "socket",
    "tcp",
    "little-endian"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.19.21",
    "@vitest/ui": "^3.2.4",
    "tsx": "^4.20.6",
    "typescript": "^5.3.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "pino": "^10.0.0",
    "pino-pretty": "^13.1.2"
  }
}
